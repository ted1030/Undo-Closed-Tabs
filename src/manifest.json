{
    "name": "Undo Closed Tabs",
    "version": "1.1",
    "description": "A simple extension that restores closed tabs.",
    "manifest_version": 3,
    "permissions": [
        "tabs",
        "sessions",
        "contextMenus"
    ],
    "background": {
        "service_worker": "background.js"
    },
    "action": {
        "default_title": "Undo Closed Tabs"
    },
    "icons": {
        "48": "48.png",
        "128": "128.png"
    }
}